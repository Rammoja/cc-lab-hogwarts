<h2>All Students</h2>

<section>
  <table>
    <thead>
      <tr>
        <td>First Name</td>
        <td>Last Name</td>
        <td>House</td>
        <td>Age</td>
        <td>Actions</td>
      </tr>
    </thead>
    <tbody>
      <% @students.each { |student| %>
      <tr>
        <td><%= student.first_name %></td>
        <td><%= student.second_name %></td>
        <td><%= student.house_name %></td>
        <td><%= student.age %></td>
        <td>
          <button>
            Edit
          </button>
          <button>
            Delete
          </button>
        </td>
      </tr>
      <% } %>
    </tbody>
  </table>
</section>